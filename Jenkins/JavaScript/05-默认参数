// 默认参数

// ES5实现方式
function getName(name) {
    var newName = name || '张三';
    console.log(newName);
}
getName();

function getName1(name) {
    var newName = name !== undefined ? name: '张三';
    console.log(newName);
}
getName1(undefined);


// ES6实现方式
function getName2(name='张三')  {
    console.log(name);
}
getName2();
